net: "models/cats_and_dogs/train_val_batch_size_16_additional_layer.prototxt"
test_iter: 60
test_interval: 500
# lr for fine-tuning should be lower than when starting from scratch
base_lr: 0.001
lr_policy: "step"
gamma: 0.2
# stepsize should also be lower, as we're closer to being done
stepsize: 2000
display: 50
max_iter: 10000
momentum: 0.8
weight_decay: 0.0005
snapshot: 1000
snapshot_prefix: "models/cats_and_dogs/train_val_batch_size_16_additional_layer"
# uncomment the following to default to CPU mode solving
# solver_mode: CPU
